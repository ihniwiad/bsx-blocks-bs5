{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bsx-blocks/container",
	"version": "0.1.0",
	"title": "BSX Container",
	"category": "widgets",
	"description": "Container with device size specific content column width.",
	"example": {},
	"attributes": {
		"isFluid": {
			"type": "boolean",
			"default": false
		},
		"containerBreakpoint": {
			"type": "string",
			"default": ""
		},
		"belowNavbar": {
			"type": "boolean",
			"default": false
		},
		"margin": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"s": { "type": "string" },
					"p": { "type": "array", "items": { "type": "string" } },
					"v": { "type": "string" }
				}
			}
		},
		"paddingBefore": {
			"type": "string",
			"default": ""
		},
		"paddingAfter": {
			"type": "string",
			"default": ""
		},
		"paddingLeft": {
			"type": "string"
		},
		"paddingRight": {
			"type": "string"
		},
		"sized": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"className": false
	},
	"textdomain": "bsx-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
