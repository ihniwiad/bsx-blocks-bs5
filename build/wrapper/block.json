{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "bsx-blocks/wrapper",
  "version": "0.1.0",
  "title": "BSX Wrapper",
  "category": "layout",
  "description": "Block to add styling (color, background, inline floating, etc.) or functionality (data attributes, gallery parent, etc.).",
  "example": {},
  "attributes": {
    "nodeName": {
      "type": "string",
      "default": "div"
    },
    "id": {
      "type": "string",
      "default": ""
    },
    "dataFn": {
      "type": "string"
    },
    "dataFnOptions": {
      "type": "string"
    },
    "dataFnTarget": {
      "type": "string"
    },
    "dataTg": {
      "type": "string"
    },
    "belowNavbar": {
      "type": "boolean"
    },
    "bgColor": {
      "type": "string"
    },
    "textSize": {
      "type": "string"
    },
    "textShadow": {
      "type": "string"
    },
    "fontWeight": {
      "type": "string"
    },
    "inverseTextColor": {
      "type": "boolean"
    },
    "headingInheritTextColor": {
      "type": "boolean"
    },
    "headingInheritFontWeight": {
      "type": "boolean"
    },
    "rounded": {
      "type": "boolean",
      "default": false
    },
    "textColor": {
      "type": "string"
    },
    "margin": {
      "type": "object",
      "default": {},
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "minItems": 4,
        "maxItems": 4
      }
    },
    "padding": {
      "type": "object",
      "default": {},
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "minItems": 4,
        "maxItems": 4
      }
    },
    "textAlign": {
      "type": "object",
      "default": {},
      "additionalProperties": {
        "type": "string"
      }
    },
    "display": {
      "type": "object",
      "default": {},
      "additionalProperties": {
        "type": "string"
      }
    },
    "border": {
      "type": "string"
    },
    "borderState": {
      "type": "string"
    },
    "width": {
      "type": "string"
    },
    "height": {
      "type": "string"
    },
    "touchFooter": {
      "type": "boolean"
    },
    "verticalAlign": {
      "type": "string"
    },
    "speechBubble": {
      "type": "string"
    },
    "isAlert": {
      "type": "boolean"
    },
    "state": {
      "type": "string"
    },
    "isGalleryParent": {
      "type": "boolean"
    },
    "multilayer": {
      "type": "string"
    },
    "zIndex": {
      "type": "string"
    },
    "isBannerInner": {
      "type": "boolean"
    },
    "position": {
      "type": "string"
    },
    "isTop0": {
      "type": "boolean"
    },
    "isBottom0": {
      "type": "boolean"
    },
    "isLeft0": {
      "type": "boolean"
    },
    "isRight0": {
      "type": "boolean"
    },
    "role": {
      "type": "string"
    },
    "tabindex": {
      "type": "string"
    },
    "hidden": {
      "type": "boolean"
    },
    "overflow": {
      "type": "string"
    },
    "textColumns": {
      "type": "string"
    }
  },
  "supports": {
    "html": false,
    "className": false
  },
  "textdomain": "bsx-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}